cd "$WINEPREFIX"
mkdir ".documents"
cd "dosdevices/c:/users/$USER"
for f in *
do
    if [ -h "$f" ]
    then
        mkdir "$WINEPREFIX/.documents/$f"
        rm "$f"
        ln -s "../../../.documents/$f" "$f"
    fi
done
cd "$WINEPREFIX/dosdevices"
test -f "y:" && rm "y:"
ln -s ".." "y:"
packages="%1"
for p in $packages
do
    [ -z "$p" ] && break
    ww_install_$p
done
